#include <stdio.h>

int main() {
    int i, j;

    // Loop through the rows (1 to 5)
    for (i = 1; i <= 5; i++) {
        // Loop through the columns (1 to 5)
        for (j = 1; j <= 5; j++) {
            // Print the current pair (i, j)
            printf("%d %d  ", i, j);
        }
        // Move to the next line after each row
        printf("\n");
    }

    return 0;
}
